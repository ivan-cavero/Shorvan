{
	"$schema": "https://biomejs.dev/schemas/1.6.2/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
	  "enabled": true,
	  "formatWithErrors": false,
	  "indentStyle": "tab",
	  "indentWidth": 2,
	  "lineEnding": "lf",
	  "lineWidth": 80,
	  "attributePosition": "auto"
	},
	"linter": { "enabled": true },
	"javascript": {
	  "formatter": {
		"jsxQuoteStyle": "single",
		"quoteProperties": "asNeeded",
		"trailingComma": "none",
		"semicolons": "asNeeded",
		"arrowParentheses": "asNeeded",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"quoteStyle": "single",
		"attributePosition": "auto"
	  }
	}
  }